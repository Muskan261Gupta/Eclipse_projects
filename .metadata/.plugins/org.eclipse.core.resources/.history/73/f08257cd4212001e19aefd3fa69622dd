package demo;

public class Program {
	
	public static void main() {
		int num=5;
		int m=1;
		int n;
		while(m<=num) {
			n=1;
			while(n++<=num-m) {
				System.out.println("*");
			}
			n=1;
			while(n++<=m*2-1) {
				System.out.println("*");
			}
			System.out.println();
			m++;
		}
		m=n-1;
		while(m>0) {
			n=1;
			while(n++<=num-m) {
				System.out.println(" ");
			}
			n=1;
			while(n++<=m*2-1) {
				System.out.println("*");
			}
			System.out.println();
			m--;
		}
	}

}
